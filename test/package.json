{
  "name": "xregistry-tests",
  "version": "1.0.0",
  "description": "Centralized test suite for xRegistry package registries",
  "private": true,  "scripts": {
    "test": "timeout 180s mocha --recursive --timeout 120000 --exit || true",
    "test:all": "timeout 180s mocha --recursive **/*.test.js --timeout 120000 --exit || true",
    "test:npm": "timeout 180s mocha npm/*.test.js --timeout 120000 --exit",
    "test:npm:angular": "timeout 180s mocha npm/integration-angular.test.js --timeout 120000 --exit",
    "test:npm:basic": "timeout 180s mocha npm/basic-server.test.js --timeout 120000 --exit",
    "test:pypi": "timeout 180s mocha pypi/*.test.js --timeout 120000 --exit",
    "test:maven": "timeout 180s mocha maven/*.test.js --timeout 120000 --exit",
    "test:nuget": "timeout 180s mocha nuget/*.test.js --timeout 120000 --exit",
    "test:oci": "timeout 180s mocha oci/*.test.js --timeout 120000 --exit",
    "test:unit": "timeout 60s mocha unit/**/*.test.js --timeout 30000 --exit",
    "test:integration": "timeout 300s mocha integration/**/*.test.js --timeout 120000 --exit",
    "test:regression": "timeout 120s mocha regression/**/*.test.js --timeout 60000 --exit",
    "test:verbose": "timeout 180s mocha --recursive --timeout 120000 --reporter spec --exit",
    "test:watch": "mocha --recursive --timeout 120000 --watch"
  },
  "devDependencies": {
    "axios": "^1.6.0",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "xregistry",
    "test",
    "npm",
    "pypi",
    "maven",
    "nuget",
    "oci",
    "integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "semver": "^7.7.2"
  }
}
