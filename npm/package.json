{
    "name": "xregistry-npm-wrapper",
    "version": "1.0.0",
    "description": "xRegistry wrapper for NPM packages",
    "main": "dist/server.js",
    "types": "dist/server.d.ts",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "start": "node dist/server.js",
        "start:dev": "ts-node src/server.ts",
        "start:watch": "ts-node --watch src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "prestart": "npm run build"
    },
    "dependencies": {
        "express": "^4.18.2",
        "axios": "^1.6.0",
        "yargs": "^17.7.2",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "@types/jest": "^29.5.8",
        "@types/uuid": "^9.0.7",
        "@types/yargs": "^17.0.32",
        "typescript": "^5.3.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.54.0",
        "rimraf": "^5.0.5",
        "nodemon": "^3.0.1"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "xregistry",
        "npm",
        "package-registry",
        "typescript"
    ],
    "author": "Your Name",
    "license": "MIT"
}