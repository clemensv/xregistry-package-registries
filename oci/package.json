{
  "name": "xregistry-oci-wrapper",
  "version": "1.0.0",
  "description": "xRegistry API proxy for OCI container registries with full xRegistry compliance",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc && npm run copy:shared",
    "copy:shared": "node -e \"require('fs').cpSync('../shared/filter', 'dist/shared/filter', { recursive: true })\"",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "xregistry",
    "oci",
    "container",
    "registry",
    "proxy",
    "docker",
    "ghcr"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "mkdirp": "^3.0.1",
    "sanitize-filename": "^1.6.3",
    "uuid": "^9.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}